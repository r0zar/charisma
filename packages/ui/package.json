{
  "name": "@repo/ui",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "sideEffects": false,
  "files": [
    "dist/**",
    "dist",
    "tailwind.config.ts"
  ],
  "exports": {
    "./button": {
      "import": {
        "types": "./dist/es/button.d.mts",
        "default": "./dist/es/button.mjs"
      },
      "require": {
        "types": "./dist/cjs/button.d.ts",
        "default": "./dist/cjs/button.js"
      }
    },
    "./card": {
      "import": {
        "types": "./dist/es/card.d.mts",
        "default": "./dist/es/card.mjs"
      },
      "require": {
        "types": "./dist/cjs/card.d.ts",
        "default": "./dist/cjs/card.js"
      }
    },
    "./navigation-menu": {
      "import": {
        "types": "./dist/es/navigation-menu.d.mts",
        "default": "./dist/es/navigation-menu.mjs"
      },
      "require": {
        "types": "./dist/cjs/navigation-menu.d.ts",
        "default": "./dist/cjs/navigation-menu.js"
      }
    },
    "./icons": {
      "import": {
        "types": "./dist/es/icons.d.mts",
        "default": "./dist/es/icons.mjs"
      },
      "require": {
        "types": "./dist/cjs/icons.d.ts",
        "default": "./dist/cjs/icons.js"
      }
    },
    "./utils": {
      "import": {
        "types": "./dist/es/utils.d.mts",
        "default": "./dist/es/utils.mjs"
      },
      "require": {
        "types": "./dist/cjs/utils.d.ts",
        "default": "./dist/cjs/utils.js"
      }
    },
    "./tailwind": {
      "types": "./tailwind.config.ts",
      "default": "./tailwind.config.ts"
    }
  },
  "scripts": {
    "build": "bunchee",
    "dev": "bunchee --watch",
    "check-types": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "jest"
  },
  "jest": {
    "preset": "@repo/jest-presets/browser"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@repo/eslint-config": "workspace:*",
    "@repo/jest-presets": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^22.14.0",
    "bunchee": "^6.4.0",
    "eslint": "^9.24.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "postcss": "^8.4.35",
    "postcss-modules": "^6.0.0",
    "typescript": "5.8.2"
  },
  "peerDependencies": {
    "@types/react": ">=18",
    "@types/react-dom": ">=18",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "dependencies": {
    "@radix-ui/react-navigation-menu": "^1.2.9",
    "@radix-ui/react-slot": "^1.2.0",
    "autoprefixer": "^10.4.18",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.488.0",
    "postcss": "^8.4.35",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7"
  }
}