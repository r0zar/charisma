{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "lib": [
      "DOM",
      "ESNext"
    ],
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "removeComments": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "stripInternal": true,
    "target": "ES2020",
    "types": [
      "node",
      "@docusaurus/module-type-aliases",
    ]
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "docusaurus.config.ts",
    "sidebars.ts"
  ],
  "exclude": [
    "node_modules",
    ".docusaurus",
    "build"
  ]
}