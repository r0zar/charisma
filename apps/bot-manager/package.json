    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@repo/polyglot": "workspace:*",
    "@repo/tokens": "workspace:*",
    "@stacks/connect": "8.1.9",
    "@stacks/encryption": "7.1.0",
