{
  "extends": [
    "//"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "@services/prices#build",
        "blaze-sdk#build",
        "dexterity-sdk#build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ],
      "env": [
        "KV_URL",
        "KV_REST_API_URL",
        "KV_REST_API_TOKEN",
        "KV_REST_API_READ_ONLY_TOKEN",
        "TOKEN_CACHE_ENDPOINT",
        "HIRO_API_KEY",
        "OPENAI_API_KEY",
        "ADMIN_SECRET",
        "BLOB_READ_WRITE_TOKEN",
        "BLOB_BASE_URL",
        "SENTRY_AUTH_TOKEN"
      ]
    },
    "dev": {
      "dependsOn": [
        "@services/prices#build",
        "blaze-sdk#build",
        "dexterity-sdk#build"
      ]
    }
  }
}