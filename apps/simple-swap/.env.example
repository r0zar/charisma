# Copy this file to .env.local and fill in your values

# Database (required for order persistence)
DATABASE_URL=your_database_url_here

# Stacks API (optional, for better rate limits)
HIRO_API_KEY=your_hiro_api_key_here

# Authentication (if applicable)
NEXTAUTH_SECRET=your_auth_secret_here
NEXTAUTH_URL=http://localhost:3002

# Trading API Keys (if using external APIs)
ALEX_API_KEY=your_alex_api_key_here

# DEX Cache API (for token metadata and LP token information)
DEX_CACHE_API_URL=https://dex-cache-charisma.vercel.app

# Twitter Integration (for notifications)
TWITTER_API_KEY=your_twitter_api_key_here
TWITTER_API_SECRET=your_twitter_api_secret_here

# Development Environment
NODE_ENV=development