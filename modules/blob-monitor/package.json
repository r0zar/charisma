{
  "name": "@modules/blob-monitor",
  "version": "0.1.0",
  "description": "Horizontal blob storage monitoring and cost management for Vercel Blob",
  "main": "dist/cjs/index.cjs",
  "module": "dist/es/index.js",
  "types": "dist/es/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bunchee",
    "clean": "rm -rf dist",
    "dev": "bunchee --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@vercel/blob": "^0.24.1",
    "@swc/helpers": "^0.5.15"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "bunchee": "^6.5.4",
    "typescript": "^5.7.2",
    "vitest": "^3.1.3"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "keywords": [
    "vercel",
    "blob",
    "storage",
    "monitoring",
    "cost-tracking",
    "performance"
  ]
}